loginButton.addEventListener("click", function(e){
	var listWin = Ti.UI.createWindow({
		modal: true,
		backgroundColor: "#0f628b"
	});
	listWin.add(listScrollView);
	navWin.openWindow(listWin), {animate: true};
});

